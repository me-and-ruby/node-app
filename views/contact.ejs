<%- include ('header') %> 
<div class="contact ">
  <h1>Contact Us</h1>
  <form action="/contact" method="post"  enctype="text/plain" >
      <div class="form-group ">
        <label for="name">Name</label>
        <input type="text" id="name" placeholder="Enter your name" class="form-control">
      </div>
      <div class="form-group ">
        <label for="email">Name</label>
        <input type="email" id="email" placeholder="Enter your email" class="form-control">
      </div>
      <div class="form-group ">
        <label for="message">Message</label>
        <textarea type="message" id="message" id="message" placeholder="Enter your message" class="form-control" ></textarea>
      </div>
      <div class="form-group spacing ">
        <button type="submit" class="btn mybutton ">Submit</button>
      </div>
  </form>
</div>
<%- include ('footer') %> 